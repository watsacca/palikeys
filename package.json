  "scripts": {
    "ng": "ng",
    "start": "ng serve --source-map",
    "build": "ng build --prod --aot --build-optimizer",
    "test": "ng test",
    "coverageNoWatch": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node scripts/postinstall.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "clean": "rm -rf dist || rmdir /S /Q dist",
    "deploy": "firebase deploy",
    "all": "npm run clean && npm run lint && npm run coverageNoWatch && npm run build && npm run deploy"
  },
  "private": true,
  "dependencies": {
